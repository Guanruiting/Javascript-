{
  "name": "kada-school-front",
  "version": "1.0.0",
  "description": "kada-school",
  "author": "xxx",
  "private": true,
  "scripts": {
    "initnei": "nei build -k 2770c73db7c26d5c39c5882f8bc898fc -o ./mock && node build/nei -i 28220 -p src/common/const",
    "upnei": "nei update -k 2770c73db7c26d5c39c5882f8bc898fc -o ./mock -w && node build/nei -i 28220 -p src/common/const",
    "psnei": "nei server -o ./mock",
    "dev": "node --max-old-space-size=8192 node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --progress --color --profile --config build/webpack.dev.conf.js",
    "dll": "webpack --config build/webpack.dll.config.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build": "node build/build.js",
    "build-online": "node build/build.js online",
    "css": "sass --watch --scss --no-cache --unix-newlines src:src -t compressed",
    "build:nodecss": "sass src/:src/ --style compressed  ",
    "watch:nodecss": "sass --watch src/:src/ --style compressed "
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": "eslint --ext .js,.vue src --fix",
    "src/**/*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
